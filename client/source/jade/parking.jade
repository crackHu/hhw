doctype html
html(lang='zh-cmn-Hans')
  include ./source/jade/common/html_head_f
  script.
    $(document).ready(function () {
      // wx_config1()
      query_parking_detail();
      $('.park_detail .action').on('touchend', function(e) {
        parking_action();
      })
      $(document).on('touchend', '.park_detail .add_car', function(e) {
          parking_add_car();
      })
      $(document).on('touchend', '.park_info .address', function(e) {
          gen_map();
      })
      $(document).on('touchend', '.map_close', function(e) {
          $('.map_wapper').fadeOut();
          $('.back_button').show();
      })

    });
  body(ontouchstart)
